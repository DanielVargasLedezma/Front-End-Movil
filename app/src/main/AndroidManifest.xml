<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.app_matricula_movil"
          android:targetSandboxVersion="1">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@drawable/_079310"
            android:label="@string/app_name"
            android:roundIcon="@drawable/_079310"
            android:supportsRtl="true"
            android:theme="@style/Theme.AppMatriculaMovil"
            android:usesCleartextTraffic="true">
        <activity
                android:name=".ui.view.NavdrawActivity"
                android:exported="true"
                android:label="@string/title_activity_navdraw"
                android:theme="@style/Theme.AppMatriculaMovil.NoActionBar"/>

        <activity
                android:name=".ui.view.MainActivity"
                android:exported="true"
                android:label="@string/app_name"
                android:theme="@style/Theme.AppMatriculaMovil.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>